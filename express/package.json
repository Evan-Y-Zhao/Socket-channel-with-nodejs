{
  "name": "sidecar-node-app",
  "version": "1.0.0",
  "repository": "",
  "author": "Evan Zhao",
  "license": "MIT",
  "main": "dist/index.js",
  "engines": {
    "node": "~6.9.1",
    "npm": ">=3.10.0"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "dev": "nodemon src/index.js --exec \"node -r babel-register\"",
    "clean": "rimraf dist",
    "build": "npm run clean && mkdir dist && babel src -s -D -d dist",
    "test": "jest --watch",
    "lint": "esw -w src test"
  },
  "dependencies": {
    "amqplib": "^0.5.2",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "bluebird": "^3.5.3",
    "body-parser": "^1.18.2",
    "cross-env": "^5.2.0",
    "es6-promise": "^4.2.5",
    "express": "^4.16.2",
    "fetch": "^1.1.0",
    "http": "0.0.0",
    "isomorphic-fetch": "^2.2.1",
    "module-alias": "^2.1.0",
    "morgan": "^1.9.0",
    "multer": "^1.4.1",
    "object-assign": "^4.1.1",
    "postal": "^2.0.5",
    "pug": "^2.0.0-beta11",
    "puppeteer": "^1.17.0",
    "rimraf": "^2.6.2",
    "socket.io": "^2.5.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.3",
    "babel-jest": "^21.2.0",
    "babel-register": "^6.26.0",
    "dotenv": "^4.0.0",
    "eslint": "^4.12.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.3.2",
    "eslint-watch": "^3.1.3",
    "jest": "^21.2.1",
    "nodemon": "^1.12.1",
    "supertest": "^3.0.0"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "sourceMaps": true,
    "retainLines": true,
    "plugins": [
      "transform-object-rest-spread",
      "transform-class-properties"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "plugins": [
      "import",
      "jest"
    ],
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module"
    },
    "env": {
      "node": true,
      "jest": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "rules": {
      "jest/no-focused-tests": 2,
      "jest/no-identical-title": 2
    }
  },
  "jest": {
    "testEnvironment": "node"
  }
}
